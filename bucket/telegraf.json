    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://dl.influxdata.com/telegraf/releases/telegraf-1.36.1_windows_amd64.zip",
            "hash": "e0a2341040e4d386c636dc1bb479fbc3c695876e9769a2160d4fec7dbee2d7fc"
        },
        "32bit": {
            "url": "https://dl.influxdata.com/telegraf/releases/telegraf-1.36.1_windows_i386.zip",
