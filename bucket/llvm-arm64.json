{
    "version": "22.1.0",
    "description": "Collection of modular and reusable compiler and toolchain technologies. (arm64/aarch64 only)",
    "homepage": "https://www.llvm.org",
    "license": "NCSA",
    "architecture": {
        "arm64": {
            "url": "https://gh-proxy.com/https://github.com/llvm/llvm-project/releases/download/llvmorg-22.1.0/LLVM-22.1.0-woa64.exe#/dl.7z",
            "hash": "7ee9bcf18d69bcf23aa8bae631baa18388f50dfb92ee08361a5b956963e38009"
        }
    },
    "pre_install": "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall.exe\" -Recurse",
    "env_add_path": "bin",
    "env_set": {
        "LIBCLANG_PATH": "$dir\\bin",
        "LLVM_LIB_DIR": "$dir\\lib"
    },
    "checkver": {
        "github": "https://github.com/llvm/llvm-project",
        "regex": "tag/llvmorg-([\\d._]+)"
    },
    "autoupdate": {
        "architecture": {
            "arm64": {
                "url": "https://gh-proxy.com/https://github.com/llvm/llvm-project/releases/download/llvmorg-$version/LLVM-$version-woa64.exe#/dl.7z"
            }
        }
    }
}
