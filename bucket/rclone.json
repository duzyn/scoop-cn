                "extract_dir": "rclone-v$version-windows-386"
            },
            "arm64": {
                "url": "https://gh-proxy.com/https://github.com/rclone/rclone/releases/download/v$version/rclone-v$version-windows-arm64.zip",
                "extract_dir": "rclone-v$version-windows-arm64"
            }
        },
        "hash": {
            "url": "$baseurl/SHA256SUMS"
        }
    }
