{
    "version": "8.6.0",
    "description": "A fast, scalable, multi-language and extensible build system",
    "homepage": "https://bazel.build",
    "license": "Apache-2.0",
    "suggest": {
        "MSYS2": "msys2",
        "Python27": "python27",
        "curl": "curl"
    },
    "architecture": {
        "64bit": {
            "url": "https://gh-proxy.com/https://github.com/bazelbuild/bazel/releases/download/8.6.0/bazel-8.6.0-windows-x86_64.zip",
            "hash": "e5bc1c928d33f6544ee7787d8811d5d0f4cff8b76fbb63c4157c2d9512eb60fc"
        }
    },
    "bin": "bazel.exe",
    "env_set": {
        "BAZEL_SH": "$(appdir msys2 $global)\\current\\usr\\bin\\bash.exe"
    },
    "checkver": {
        "github": "https://github.com/bazelbuild/bazel"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://gh-proxy.com/https://github.com/bazelbuild/bazel/releases/download/$version/bazel-$version-windows-x86_64.zip"
            }
        },
        "hash": {
            "mode": "extract",
            "url": "$url.sha256"
        }
    }
}
