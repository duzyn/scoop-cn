{
    "version": "1.25.1",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.nju.edu.cn/golang/go1.25.1.windows-amd64.zip",
            "hash": "4a974de310e7ee1d523d2fcedb114ba5fa75408c98eb3652023e55ccf3fa7cab"
        },
        "32bit": {
            "url": "https://mirrors.nju.edu.cn/golang/go1.25.1.windows-386.zip",
            "hash": "be13d5479b8c75438f2efcaa8c191fba3af684b3228abc9c99c7aa8502f34424"
        },
        "arm64": {
            "url": "https://mirrors.nju.edu.cn/golang/go1.25.1.windows-arm64.zip",
            "hash": "04bc3c078e9e904c4d58d6ac2532a5bdd402bd36a9ff0b5949b3c5e6006a05ee"
        }
    },
    "extract_dir": "go",
    "installer": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Adding '$envgopath\\bin' to PATH...\"",
            "Add-Path -Path \"$envgopath\\bin\" -Global:$global -Force"
        ]
    },
    "uninstaller": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Removing '$envgopath\\bin' from PATH...\"",
            "Remove-Path -Path \"$envgopath\\bin\" -Global:$global"
        ]
    },
    "bin": [
        "bin\\go.exe",
        "bin\\gofmt.exe"
    ],
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.nju.edu.cn/golang/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://mirrors.nju.edu.cn/golang/go$version.windows-386.zip"
            },
            "arm64": {
                "url": "https://mirrors.nju.edu.cn/golang/go$version.windows-arm64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
